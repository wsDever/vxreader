{
  	"name": "vxreader",
	"displayName": "vxReader",
	"description": "A vx reader tool in vscode",
	"version": "0.0.1",
	"publisher": "wushang",
	"keywords":["vscode","web","plugin","read", "vx"],
	"engines": {
		"vscode": "^1.46.0"
	},
	"icon": "logo.png",
    "repository": {
        "type": "git",
		"url": "https://github.com/wsDever/vxreader.git"
    },
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:vxread.openVxRead"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "vxread.openVxRead",
            "title": "VXReader",
			"icon": "./logo.png"
		}],
		"menus": {
			"editor/context": [
				{
					"command": "vxread.openVxRead"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.46.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    }

}
